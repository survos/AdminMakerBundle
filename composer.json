{
    "name": "survos/maker-bundle",
    "description": "Tools to generate controllers and templates for the admin interface",
    "type": "symfony-bundle",
    "keywords": ["generator", "symfony", "crud"],
    "require": {
        "php": "^8.1",
        "symfony/config": "^6.1",
        "symfony/console": "^6.1.0",
        "symfony/options-resolver": "^6.1.0",
        "symfony/string": "^6.1.0",
        "symfony/workflow": "^6.1.0",
        "twig/twig": "^3.4"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/orm": "^2.12",
        "haydenpierce/class-finder": "^0.4.3",
        "phpstan/phpstan": "^1.8",
        "rector/rector": "^0.13.9",
        "sensio/framework-extra-bundle": "^6.2",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/form": "^6.1.0",
        "symfony/http-client": "^6.1.0",
        "symfony/maker-bundle": "^1.38",
        "symfony/phpunit-bridge": "^6.1.0",
        "symfony/polyfill-php80": "^1.16.0",
        "symfony/process": "^6.1.0",
        "symfony/security-core": "^6.1.0",
        "symfony/security-csrf": "^6.1.0",
        "symfony/twig-bundle": "^6.1.0",
        "symfony/validator": "^6.1.0",
        "symfony/yaml": "^6.1.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text --coverage-clover=build/coverage.xml coverage",
        "test-coverage": "php -d xdebug.mode=coverage vendor/bin/phpunit --coverage-html build/coverage",
        "test-phpstan": "vendor/bin/phpstan analyse"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "Survos\\Bundle\\MakerBundle\\": "src"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-main": "1.3-dev"
        },
        "symfony": {
            "require": "^6.1.0",
            "allow-contrib": "true",
            "id": "maker-bundle.survos.com"
        }
    },
    "conflict": {
        "survos/html-prettify-bundle": "<1.2.5",
        "survos/barcode-bundle": "<1.2.5",
        "survos/api-grid-bundle": "<1.2.5",
        "survos/second-package": "<1.2.5",
        "survos/bootstrap-bundle": "<1.2.5",
        "survos/inspection-bundle": "<1.2.5",
        "survos/faker-bundle": "<1.2.5",
        "survos/doc-bundle": "<1.2.5",
        "survos/grid-bundle": "<1.2.5",
        "survos/core-bundle": "<1.2.5",
        "survos/tree-bundle": "<1.2.5",
        "survos/first-package": "<1.2.5",
        "survos/auth-bundle": "<1.2.5",
        "survos/workflow-helper-bundle": "<1.2.5",
        "survos/crawler-bundle": "<1.2.5"
    },
    "prefer-stable": true
}
